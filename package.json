{
  "name": "create-nodejs-express-app",
  "version": "1.7.0",
  "description": "Create a Node.js app for building production-ready RESTful APIs using Express, by running one command",
  "bin": "bin/createNodejsApp.js",
  "main": "src/index.js",
  "repository": "https://github.com/hagopj13/node-express-boilerplate.git",
  "author": "Dinesh <dinesh@neurindustries.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix **/*.js",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.503.1",
    "aws-sdk": "^2.1552.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^7.1.5",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "^0.0.1-security",
    "helmet": "^7.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.0.3",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pm2": "^5.3.0",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^5.0.0",
    "three": "^0.164.1",
    "three-stdlib": "^2.30.0",
    "tmp": "^0.2.3",
    "unirest": "^0.6.0",
    "uuid": "^9.0.1",
    "validator": "^13.0.0",
    "websocket": "^1.0.34",
    "winston": "^3.2.1",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5",
    "xlsx-style": "^0.8.13",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "coveralls": "^3.0.7",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.0",
    "faker": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.0.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
